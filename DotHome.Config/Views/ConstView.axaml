<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 xmlns:local="clr-namespace:DotHome.Config.Views;assembly=DotHome.Config"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="DotHome.Config.Views.ConstView"
			 Padding="2" Background="Gray">
	<Grid Background="#999999" ColumnDefinitions="*,*">
		<TextBlock x:Name="textBlock" Grid.Column="0"/>
		<local:OutputView x:Name="outputView" Grid.Column="1"/>
	</Grid>
</UserControl>
